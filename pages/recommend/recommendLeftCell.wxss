
.leftCell {
    height: 90rpx;
    border-bottom: 3rpx solid #f8f7f7;
}
.leftMargin {
    background-color: #f1f1f1 ;
    width: 6rpx;
    display: inline-block;
    
    height: 100%;
    vertical-align: middle;
}
.leftTitle {
    color: #777777;
    margin-left: 24rpx;
    font-size: 30rpx;
    height: 100%;
}

/*被选中*/
.leftCellSelected .leftMargin {
    background-color: #ff3b30;
}
/*被选中的文本颜色*/
.leftCellSelected .leftTitle {
    color: #e42e16;
}